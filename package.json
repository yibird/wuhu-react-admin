{
  "name": "wuhu-react-admin",
  "version": "0.0.0",
  "private": true,
  "description": "一个基于React18、ReactRouter、Zustand、Antd5、TypeScript实现快速开发平台",
  "keywords": [
    "react",
    "react18",
    "typeScript",
    "reactRouter",
    "zustand",
    "ant design"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yibird/wuhu-react-admin"
  },
  "license": "MIT",
  "author": {
    "name": "zchengfeng",
    "email": "2684837849@qq.com",
    "url": "https://github.com/yibird"
  },
  "type": "module",
  "scripts": {
    "bootstrap": "pnpm install",
    "build": "cross-env NODE_ENV=production tsc && pnpm vite build",
    "build:no-cache": "pnpm clean:cache && npm run build",
    "dev": "pnpm vite",
    "lint": "turbo run lint",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{ts,tsx}\" --fix",
    "lint:prettier": "prettier --write .",
    "prepare": "husky",
    "preview": "vite preview",
    "serve": "npm run dev",
    "type:check": "vue-tsc --noEmit --skipLibCheck"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.{scss,less,styl,html}": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  },
  "dependencies": {
    "@ant-design/charts": "^2.0.3",
    "@antv/g2": "^5.1.15",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^7.0.2",
    "@dnd-kit/utilities": "^3.2.2",
    "@formkit/auto-animate": "1.0.0-beta.5",
    "@loadable/component": "^5.16.3",
    "@logicflow/core": "^1.2.22",
    "@logicflow/extension": "^1.2.22",
    "@tanstack/react-query": "^4.36.1",
    "ahooks": "^3.7.8",
    "antd": "^5.15.0",
    "auto-zustand-selectors-hook": "^2.0.0",
    "axios": "^1.6.7",
    "clsx": "^1.2.1",
    "dayjs": "^1.11.10",
    "immer": "^9.0.21",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "prettier-plugin-packagejson": "^2.4.8",
    "react": "^18.2.0",
    "react-activation": "^0.12.4",
    "react-dom": "^18.2.0",
    "react-intl": "^6.5.5",
    "react-router-dom": "^6.21.1",
    "react-tracked": "^1.7.13",
    "react-transition-group": "^4.4.5",
    "remixicon": "^4.2.0",
    "scheduler": "^0.23.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "@commitlint/cz-commitlint": "^19.0.3",
    "@commitlint/types": "^19.0.3",
    "@faker-js/faker": "^8.3.1",
    "@release-it/conventional-changelog": "^5.1.1",
    "@types/loadable__component": "^5.13.8",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^18.19.3",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/react-transition-group": "^4.4.10",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@unocss/preset-rem-to-px": "^0.58.2",
    "@unocss/preset-uno": "^0.58.2",
    "@unocss/preset-wind": "^0.58.2",
    "@unocss/transformer-directives": "^0.58.2",
    "@unocss/transformer-variant-group": "^0.58.2",
    "@vitejs/plugin-legacy": "^5.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "faker": "^6.6.6",
    "husky": "^9.0.11",
    "less": "^4.2.0",
    "lint-staged": "^15.2.2",
    "oxlint": "^0.2.13",
    "prettier": "^3.2.5",
    "release-it": "^17.1.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "terser": "^5.26.0",
    "turbo": "^1.11.2",
    "typescript": "^5.3.3",
    "unocss": "^0.58.5",
    "vite": "^5.1.5",
    "vite-plugin-compression2": "^0.11.0",
    "vite-plugin-dynamic-import": "^1.5.0",
    "vite-plugin-external-cdn": "^1.0.1",
    "vite-plugin-image-optimizer": "^1.1.7",
    "vite-plugin-mock-dev-server": "^1.4.7",
    "vite-plugin-remove-console": "^2.2.0"
  }
}
